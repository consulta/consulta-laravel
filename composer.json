{
  "name": "consulta/laravel",
  "description": "Paquete de Laravel para validación de RUC y DNI en Perú",
  "license": "MIT",
  "keywords": [
    "reniec",
    "sunat",
    "php",
    "dni",
    "ruc",
    "validar",
    "validate",
    "consultar",
    "consulta dni",
    "consulta ruc",
    "consulta sunarp",
    "consulta vehicular",
    "consulta dni sin codigo captcha",
    "consulta ruc sin codigo captcha",
    "captcha",
    "framework",
    "laravel"
  ],
  "autoload": {
    "psr-4": {
      "Consulta\\Laravel\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Consulta\\Laravel\\Test\\": "tests"
    }
  },
  "require": {
    "php": "^7.2",
    "guzzlehttp/guzzle": "~6.0 || ^7.0 || ^8.0",
    "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.*|| 5.9.* || ^6.0 || ^7.0 || ^8.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Consulta\\Laravel\\ConsultaServiceProvider"
      ]
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3",
    "orchestra/testbench": "^4.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
